<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="IousAzure.Principal" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="IousAzure.Website" vmsize="Small">
    <Sites>
      <Site name="Web">
        <VirtualApplication name="IousAzure.Servico" physicalDirectory="../IousAzure.Servico" />
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
  </WebRole>
</ServiceDefinition>